      "source": [
        "j = 0\n",
        "while j<100000:\n",
        "    s = 0\n",
        "    n = str(j)\n",
        "    for i in n:\n",
        "        s+=int(i)**len(n)\n",
        "    if s==j:\n",
        "        print(j)\n",
        "    j+=1"
      ],
